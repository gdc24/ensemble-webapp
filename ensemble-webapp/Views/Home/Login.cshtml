@model ensemble_webapp.ViewModels.LoginVM

@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>

<p>
    username & password form
</p>

@using (Html.BeginForm("LoginUser", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <p>
        @Html.EditorFor(m => m.logInUser.StrUsername)
        @Html.EditorFor(m => m.logInUser.StrPassword)
    </p>
}

